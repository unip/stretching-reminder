{
  "name": "stretching-reminder",
  "version": "1.0.0",
  "description": "Desktop app that reminds you to stretch during work hours",
  "author": "",
  "homepage": "https://github.com/stretching-reminder/stretching-reminder",
  "type": "module",
  "main": "dist/main/index.cjs",
  "scripts": {
    "dev": "vite",
    "build": "npm run build:main && npm run build:preload && npm run build:renderer && cp package.json dist/ && npx electron-builder",
    "build:main": "vite build --config vite.config.main.js",
    "build:preload": "vite build --config vite.config.preload.js",
    "build:renderer": "vite build",
    "build:app": "npm run build:main && npm run build:preload && npm run build:renderer",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:e2e": "playwright test",
    "preview": "vite preview",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,css,json}\"",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "stretching",
    "reminder",
    "health",
    "electron"
  ],
  "license": "ISC",
  "devDependencies": {
    "@eslint/compat": "^2.0.2",
    "@playwright/test": "^1.58.2",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/auto-launch": "^5.0.5",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.56.1",
    "@typescript-eslint/parser": "^8.56.1",
    "@vitejs/plugin-react": "^4.6.0",
    "autoprefixer": "^10.4.21",
    "electron": "^40.6.1",
    "electron-builder": "^25.1.8",
    "eslint": "^9.39.3",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "fs-extra": "^11.3.3",
    "jsdom": "^26.1.0",
    "playwright": "^1.52.0",
    "postcss": "^8.5.3",
    "prettier": "^3.8.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "sharp": "^0.34.5",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.56.1",
    "vite": "^6.3.5",
    "vitest": "^3.2.4",
    "zustand": "^5.0.5"
  },
  "dependencies": {
    "auto-launch": "^5.0.6",
    "electron-store": "^8.2.0"
  }
}
